#%%
# Least common multiplier 
def EuclideanLCM(x, y):

    if 'x_lst' not in globals():
        global x_lst
        x_lst = []
    if 'y_lst' not in globals():
        global y_lst
        y_lst = []

    if 0 not in y_lst:
        x_lst.append(x)
        y_lst.append(y)

    if y == 0:
        return x * (x_lst[0]/x) * (y_lst[0]/x)

    return EuclideanLCM(y, x % y)

# %%
